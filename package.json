{
  "name": "typegoose-cursor-pagination",
  "version": "1.0.1",
  "description": "A mongoose plugin to find results using on cursor-based pagination with support for typegoose",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json -d",
    "test": "ava",
    "lint": "tslint -c ./tslint.json './src/**/*.ts'",
    "ci": "npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtraBB/typegoose-cursor-pagination.git"
  },
  "keywords": [
    "mongodb",
    "mongo",
    "paging",
    "pagination",
    "cursor",
    "mongoose",
    "plugin",
    "typegoose"
  ],
  "author": "Bruno Carvalhal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ExtraBB/typegoose-cursor-pagination/issues"
  },
  "homepage": "https://github.com/ExtraBB/typegoose-cursor-pagination#readme",
  "dependencies": {
    "base64-url": "^2.2.0",
    "mongodb-extended-json": "^1.10.1",
    "mongoose": "^5.4.14",
    "typegoose": "^5.5.0"
  },
  "devDependencies": {
    "@types/base64-url": "^2.2.0",
    "@types/mongoose": "^5.3.19",
    "@types/node": "^11.9.4",
    "ava": "^1.2.1",
    "ava-spec": "^1.1.1",
    "mongodb-memory-server": "^3.1.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
