{
    "name": "typegoose-cursor-pagination",
    "version": "1.2.2",
    "description": "A mongoose plugin to find results using on cursor-based pagination with support for typegoose",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc -p tsconfig.json -d",
        "test": "ava",
        "lint": "tslint -c ./tslint.json './src/**/*.ts'",
        "ci": "npm run lint && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ExtraBB/typegoose-cursor-pagination.git"
    },
    "keywords": [
        "mongodb",
        "mongo",
        "paging",
        "pagination",
        "cursor",
        "mongoose",
        "plugin",
        "typegoose"
    ],
    "author": "Bruno Carvalhal",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ExtraBB/typegoose-cursor-pagination/issues"
    },
    "homepage": "https://github.com/ExtraBB/typegoose-cursor-pagination#readme",
    "dependencies": {
        "@typegoose/typegoose": "8.0.0-beta.5",
        "base64-url": "^2.2.0",
        "bson-ext": "^2.0.5",
        "mongodb-extended-json": "^1.11.0",
        "mongoose": "^5.12.4",
        "ramda": "^0.27.0"
    },
    "devDependencies": {
        "@types/base64-url": "^2.2.0",
        "@types/node": "^13.9.5",
        "@types/ramda": "0.25.24",
        "ava": "^3.5.1",
        "ava-spec": "^1.1.1",
        "mongodb-memory-server": "^6.5.0",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.0",
        "typescript": "^4.2.4"
    },
    "ava": {
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    }
}
