{
    "name": "typegoose-cursor-pagination",
    "version": "1.2.2",
    "description": "A mongoose plugin to find results using on cursor-based pagination with support for typegoose",
    "main": "lib/index.js",
    "scripts": {
        "build": "tsc -p tsconfig.json -d",
        "test": "ava",
        "lint": "tslint -c ./tslint.json './src/**/*.ts'",
        "ci": "npm run lint && npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ExtraBB/typegoose-cursor-pagination.git"
    },
    "keywords": [
        "mongodb",
        "mongo",
        "paging",
        "pagination",
        "cursor",
        "mongoose",
        "plugin",
        "typegoose"
    ],
    "author": "Bruno Carvalhal",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ExtraBB/typegoose-cursor-pagination/issues"
    },
    "homepage": "https://github.com/ExtraBB/typegoose-cursor-pagination#readme",
    "dependencies": {
        "@typegoose/typegoose": "^9.7.0",
        "base64-url": "^2.3.3",
        "mongodb-extended-json": "^1.11.1",
        "mongoose": "^6.2.4",
        "ramda": "^0.28.0"
    },
    "devDependencies": {
        "@types/base64-url": "^2.2.0",
        "@types/mongoose": "^5.11.97",
        "@types/node": "^17.0.21",
        "@types/ramda": "^0.28.0",
        "ava": "^4.0.1",
        "ava-spec": "^1.1.1",
        "mongodb-memory-server": "^8.4.0",
        "ts-node": "^10.6.0",
        "tslint": "^6.1.3",
        "typescript": "^4.6.2"
    },
    "ava": {
        "extensions": [
            "ts"
        ],
        "require": [
            "ts-node/register"
        ]
    }
}
